@model IEnumerable<MVC12.Models.Employee>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "CreateEmployee")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EmployeeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Gender)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Qualification)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LOcation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Department)
        </th>
        <th></th>
    </tr>

    @foreach (var employee in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(x => employee.EmployeeName)
            </td>
            <td>
                @Html.DisplayFor(x => employee.Gender)
            </td>
            <td>
                @Html.DisplayFor(x => employee.Qualification)
            </td>-
            <td>
                @Html.DisplayFor(x => employee.LOcation)
            </td>

            <td>
                @Html.ActionLink("Edit", "Edit", new { id = employee.Id }) |
                @Html.ActionLink("Details", "GetEmployeesById", new { id = employee.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = employee.Id })
            </td>
        </tr>
    }

</table>